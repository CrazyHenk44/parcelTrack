# Example Environment variables for ParcelTrack
# Copy this file to .env and fill in your details.

# The public URL for your application, used in email links.
PARCELTRACK_URL=http://localhost:8080

# Default email for notifications if not specified per package.
DEFAULT_EMAIL=your_email@example.com

# Default country code for shipments (ISO 2-letter code).
DEFAULT_COUNTRY=NL

# SMTP settings for sending emails via msmtp.
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your_smtp_username
SMTP_PASS=your_smtp_password
SMTP_FROM=parceltrack@example.com

# Log level for the application (e.g., DEBUG, INFO, ERROR). Defaults to INFO if not set.
LOG_LEVEL=INFO

# Ship24 API Key (optional). If provided, enables Ship24 as a shipper.
SHIP24_API_KEY=